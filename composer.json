    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "araujolops",
            "email": "araujolops@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "phpmailer/phpmailer": "6.0.1"
